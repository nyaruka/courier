{
    "name": "Courier",
    "image": "mcr.microsoft.com/devcontainers/go:dev-1.25-trixie",
    "features": {},
    "runArgs": [
        "--name=dev-courier",
        "--network=textit_default"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "netcorext.uuid-generator"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "zsh",
                "editor.formatOnSave": true,
                "editor.rulers": [
                    120
                ]
            }
        }
    }
}